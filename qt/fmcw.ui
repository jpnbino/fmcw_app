                       <property name="toolTip">
                        <string>&lt;html&gt;&lt;head/&gt;&lt;body&gt;&lt;p&gt;&lt;span style=&quot; font-weight:700;&quot;&gt;Cell Fail Fault (CELLF)&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot; font-weight:700;&quot;&gt;Address&lt;/span&gt;: 0x81 bit4 &lt;/p&gt;&lt;p&gt;&lt;span style=&quot; font-weight:700;&quot;&gt;Set when&lt;/span&gt;: Difference between CELMIN (0x8A-8B) and CELMAX (0x8C-8D) exceeds CBMAXDV (0x22-23).&lt;/p&gt;&lt;p&gt;&lt;span style=&quot; font-weight:700;&quot;&gt;Effect&lt;/span&gt;: Turns off cell balance and power FETs. Triggers Open-Wire Scan&lt;/p&gt;&lt;p&gt;&lt;span style=&quot; font-weight:700;&quot;&gt;Cleared when&lt;/span&gt;: Voltage difference is below CBMAXDV in subsequent measurements. &lt;/p&gt;&lt;p&gt;&lt;span style=&quot; font-weight:700;&quot;&gt;Disable&lt;/span&gt;: Set CBMAXDV to 0x0FFF (disables open-wire detection).&lt;/p&gt;&lt;/body&gt;&lt;/html&gt;</string>
                       </property>
                       <property name="text">
                        <string>CELLF</string>
                       </property>
                       <property name="checked">
                        <bool>true</bool>
                       </property>
                      </widget>
